}

[ENABLE]

{$lua}

	-- ItemStartAddressOffset = 0x297470C
	-- KnowItemList = 已知物品列表, ItemTypeNameList = 物品类别名称表, ItemTypeCountList = 物品类别数量表

	-- 获取物品ID所属类型的最大数量，失败返回 0，事件物品返回 -1
	local function GetItemTypeCount( id )
		for tp, data in pairs( KnowItemList ) do
			if data[id] then
				return ItemTypeCountList[tp] or 0
			end
		end
		return 0
	end

	-- 读取已存在的全部道具数据，失败返回 nil
    -- 返回：{ id = { Value = 数量, Offset = 索引 } }， 首个可以新增的物品地址索引
	-- 索引从 0 开始
	local function ReadAllItem( bad )
		bad = bad or GetBaseAddress()
		if not bad then return nil end

		local datas = {}
		local index = 0
		local ad = bad + ItemStartAddressOffset
		while(true) do
			local id = readSmallInteger( ad )
			local val = readSmallInteger( ad + 2 )
			if id == 0 then return datas, index end

			datas[id] = {
				Value = val,
				Offset = index
			}
			index = index + 1
			ad = ad + 4
		end

	end

	-- 批量添加物品
	-- data：	{ 物品ID = 数量 }
	-- bad：	基址，可选
	local function BatchAddItem( data, bad )
		if not data or type(data) ~= "table" then
			print( "无效的物品数据！" )
			return
		end

		bad = bad or GetBaseAddress()
		if not bad then
			print( "无效的基址！" )
			return
		end

		local items, lastidx = ReadAllItem( bad )
		if not items or not lastidx then
			print( "读取已存在物品数据时错误！" )
			return
		end

		local sad = bad + ItemStartAddressOffset
		for id, count in pairs( data ) do
			if items[id] then
				local ad = sad + items[id].Offset * 0x04 + 0x02
				local val = readSmallInteger( ad )
				if count &gt; val then
					writeSmallInteger( ad, count )
				end
			else
				--print( string.format( "添加物品：Code = %04X， Count = %d", id, count ) )
				local ad = sad + lastidx * 0x04
				lastidx = lastidx + 1
				writeSmallInteger( ad, id )
				writeSmallInteger( ad + 0x02, count )
			end

		end

	end

	-- 添加所有【xx】
	local function AddItem( rcd )
		local list = {
			[3834] = { "Weapon" },						-- 添加所有【武器】
			[3841] = { "Attire", "Boots", "Orna" },		-- 添加所有【防具】
			[3840] = { "Attire" },							-- 添加所有【衣服】
			[3839] = { "Boots" },							-- 添加所有【靴子】
			[3838] = { "Orna" },							-- 添加所有【饰品】
			[3837] = { "Drug" },						-- 添加所有【药品】
			[3836] = { "Food" },						-- 添加所有【食材】
			[3835] = { "Book" },						-- 添加所有【书籍】
			[3843] = { "Fish", "FishTool" },			-- 添加所有【垂钓】
			[3842] = { "Crystals" },					-- 添加所有【结晶回路】
		}

		local rid = rcd and rcd.ID
		if not list[rid] then return end

		local bad = GetBaseAddress()
		if not bad then return end

		local data = {}
		for _, key in ipairs( list[rid] ) do
			local count = ItemTypeCountList[key] or 0
			if KnowItemList[key] and count &gt; 0 then
				for id, _ in pairs( KnowItemList[key] ) do
					data[id] = count
				end
			end
		end

		BatchAddItem( data, bad )

        print( "=================================" )
        print( string.format( "%s 完成！", rcd.Description ) )
	end

	-- 新增/修改（物品不存在时，新增物品，否则修改物品数量）
	local function AddModifyItem( rcd )
		local bad = GetBaseAddress()
		if not bad then
			print( "基址无法获取：游戏未运行，或未挂载！" )
		else
			local iad = getAddressSafe( "ItemAddModifyIdAdress" )
			local cad = getAddressSafe( "ItemAddModifyCountAdress" )

			local nid = iad and iad ~= 0 and readSmallInteger(iad) or 0
			local nval = cad and cad ~= 0 and readSmallInteger(cad) or 0
			if nid == 0 then
				print( "新增/修改失败：物品ID未选择！" )
			elseif nval &lt;= 0 then
				print( "新增/修改失败：物品数量不能为 0！" )
			elseif nval &gt; 99 then
				print( "新增/修改失败：物品数量不能大于 99！" )
			else
				local sad = bad + ItemStartAddressOffset
				local msg = ""
				while(true) do
					local id = readSmallInteger( sad )
					if id == 0 then
						msg = "新增物品成功"
						break
					elseif id == nid then
						msg = "修改数量成功"
						break
					end

					sad = sad + 4
				end

				writeSmallInteger( sad, nid )
				writeSmallInteger( sad + 2, nval )
				print( string.format( "%s：0x%04X = %d", msg, nid, nval ) )

				-- 恢复临时数据
				writeSmallInteger( iad, 0 )
				writeSmallInteger( cad, 1 )
			end

		end

	end

	-- 已存的物品数量最大化
	local function ItemCountMax( rcd )
		local bad = GetBaseAddress()
		if not bad then return end

		local ad = bad + ItemStartAddressOffset
		while(true) do
			local id = readSmallInteger( ad )
			if id == 0 then break end		-- 物品数据区尾

			local count = GetItemTypeCount( id )
			if count &gt; 0 then
				writeSmallInteger( ad + 2, count )
			end

			ad = ad + 4
		end

        print( "物品数量最大化完成！" )
	end

	-- 点击功能记录栏ID表
	local RecordList = {
		[3844] = ItemCountMax,	-- 现存物品数量最大化（不含关键道具）
		[3769] = AddModifyItem,	-- 新增/修改（物品不存在时，新增物品，否则修改物品数量）
