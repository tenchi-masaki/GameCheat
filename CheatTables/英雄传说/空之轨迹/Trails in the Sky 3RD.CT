	-- 断点处理
	local function OnBreak()
		-- 获取准备写入的物品代码
        local esi = ESI
        local id = esi &amp; 0xFFFF
        --print( string.format( "写入的物品代码：ID = %04X", id ) )

		if id &gt; 0 and not ItemList[id] then
			print( "=========================" )
			print( string.format( "未知物品：ID = %04X", id ) )
			SendSound( "未知物品" )

			ItemList[id] = 2
		end

	end

	-- 设置断点
	local bad = GetBaseAddress()
	if bad then
		-- 启用时检测已存在的物品
		local newitem = {}
		local sad = bad + ItemStartAddressOffset
		while(true) do
			local id = readSmallInteger( sad )
			if id == 0 then break end

			if not ItemList[id] then
				ItemList[id] = 1
				table.insert( newitem, string.format( "0x%04X", id ) )
			end
			sad = sad + 4
		end

		if #newitem &gt; 0 then
			print( "=========================" )
			print( string.format( "未知物品： %s", table.concat( newitem, "， " ) ) )
			SendSound( "未知物品" )
		end

		-- 设置断点
        AddBreakPoint( bad + 0x0FE562, OnBreak )
	end

