        <CheatEntry>
          <ID>7497</ID>
          <Description>"基础数据"</Description>
          <Options moManualExpandCollapse="1" moDeactivateChildrenAsWell="1"/>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>[ENABLE]
//从此处开始直到 '[DISABLE]' 的代码将被用于启用功能

{$lua}

	local list = getAddressList()
	local rcd = list and list.getMemoryRecordByID( 7497 )
	if not rcd then return end

	-- 取基址
	local bad = GetBaseAddress()
	if not bad then return end
	local sad = bad + 0x28A51AC

	local rolemax = math.min( rcd.Count, 16 ) - 1
	for i = 0, rolemax do
		local rsd = rcd[i]
		local id = readSmallInteger( sad + 0x38 )

		local adstr = string.format( "+20+3C*%02X", i )
		if rsd.Address ~= adstr then
			rsd.Address = adstr
		end

		local desc = string.format( "%02d %s", i + 1, RoleSerialList[i] or "" )
		if rsd.Description ~= desc then
			rsd.Description = desc
		end
	end

{$asm}



