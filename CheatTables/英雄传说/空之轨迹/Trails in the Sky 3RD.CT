	-- 		adlist：	[可选] getAddressList() 方法返回的值（ID参数为表时忽略此参数）
	function AddRecordClick( id, func, adlist )
		local list = adlist or getAddressList()
		if not list then return end

		-- ID参数为表时
		if type( id ) == "table" then
			for rid, rfunc in pairs( id ) do
				AddRecordClick( rid, rfunc, list )
			end
			return
		end

		if type(func) ~= "function" then return end

		local record = type(id) == "number" and list.getMemoryRecordByID( tonumber(id) ) or type(id) == "string" and list.getMemoryRecordByDescription( id ) or nil
		if record then
			-- memrec 全局变量：表示当前的项目对象
			record.OnActivate = function( rcd, before, state )
				-- 此事件会执行2次：
				-- 		第一次：before = true, 	state = false
				-- 		第二次：before = false, 	state = true
				--print( rcd.ID, tostring(rcd), tostring(before), tostring(state) )
				if before and not state then
					func( rcd )
				end
				return false
			end
		end

	end

end

do	-- 计时器对象处理

	-- 新建/获取计时器对象，失败时返回 nil
	-- 		name：		string = 计时器名称 / userdata = 计时器对象
	-- 		func：		可选，function = 执行的方法 / nil = 返回指定名称计时器对象
	-- 		interval：	可选，number = 计时间隔，默认 = 1000
	function NewTimer( name, func, interval )
		local ntp = type(name)
		local timer = ntp == "string" and _G[name] or ntp == "userdata" and name
		timer = timer and timer.ClassName == "TTimer" and timer

		if not func or type(func) ~= "function" then
			return timer
		end

		timer = timer or createTimer( getMainForm() )
		timer.Interval = type(interval) == "number" and interval &gt; 0 and interval or 1000
		timer.OnTimer = func

		if ntp == "string" then
			_G[name] = timer
		end

		return timer
	end

	-- 销毁计时器
	function DelTimer( name )
		local ntp = type(name)
		local timer = ntp == "string" and _G[name] or ntp == "userdata" and name
		timer = timer and timer.ClassName == "TTimer" and timer

		if timer then
			timer.destroy()
			if ntp == "string" then
				_G[name] = nil
			elseif ntp == "userdata" then
				name = nil
			end
		end
	end

end

do	-- 设置记录栏下拉列表项目

	-- 设置记录栏下拉列表项目
	-- 		tab = { 格式化后的数字， 显示文本 }
	function SetDropDownList( record, tab )
		if record then
			local tlist = {}
			for id, name in pairs( tab ) do
				table.insert( tlist, string.format( "%s:%s", id, name ) )
			end

			table.sort( tlist )
			local str = table.concat( tlist, "\r\n")
			if record.DropDownList.Text ~= str then
				record.DropDownList.Text = str
			end

		end
	end

end










