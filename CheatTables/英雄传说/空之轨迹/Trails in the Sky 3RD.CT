do	-- 游戏进程，必须设置内部的游戏进程名称、基址

	-- string，游戏进程名称（含后缀名）
	local ProcessName = "main.exe"
	-- string，基址计算公式，仅使用不以0x开头的十六进制数字
	-- 		nil						使用游戏进程名称（含后缀名）
	-- 		game.exe+1234			直接计算
	-- 		[game.exe+1234]+08		指针计算
	-- 		$MyGlobalsymbol			全局符号计算
	local BaseAddressStr = nil

	-- 判断游戏是否正在运行中
	function InGame()
		return process and process == ProcessName and readInteger(process) ~= nil
	end

	-- 获取游戏基址
	function GetBaseAddress()
		local ad = InGame() and getAddressSafe( BaseAddressStr or ProcessName )
		return ad and ad ~= 0 and ad or nil
	end

end

