        <CheatEntry>
          <ID>7505</ID>
          <Description>"队伍成员"</Description>
          <Options moManualExpandCollapse="1" moDeactivateChildrenAsWell="1"/>
          <VariableType>Auto Assembler Script</VariableType>
          <AssemblerScript>{ 游戏   : ed6_win2_DX9_cn.exe
  版本:
  日期   : 2024-01-27
  作者 : PP

  刷新队伍角色数据，无法拦截到载入时的数据变动
  所以使用计时器刷新
}

[ENABLE]

{$lua}

	local list = getAddressList()
	local PartHeadRecord = list and list.getMemoryRecordByID( 7505 )	-- 队伍成员
	if not PartHeadRecord then return end

	-- 检索符合条件的成员记录栏
	local PartRecordList = {}
	for idx = 0, PartHeadRecord.Count - 1 do
		if PartHeadRecord[idx].IsGroupHeader then
			table.insert( PartRecordList, PartHeadRecord[idx] )
		end
	end

	-- 队伍成员变动处理
	local function PartChanged( index, id )
		local Part = PartRecordList[index]
		if not Part then return end

		if id &gt;= 0 and id &lt; 16 then
			-- 主角们，16 项 = 00 - 15
			if not RoleSerialList[id] then
				RoleSerialList[id] = string.format( "0x%02X", id  )
				print( "\r\n===================================" )
				print( string.format( "未知角色，ID = 0x%02X", id ) )
				SendSound( "未知角色" )
			end
			SetRecordDesc( Part, string.format( "%02d %s", index, RoleSerialList[id] ) )
			SetRecordOptions( Part, HeadOptions.Standard )
			SetRecordColor( Part, 0x00FF00FF )

			-- 设置子项的地址
			for i = 0, Part.Count - 1 do
				local rsd = Part[i]
				if rsd.Description == "角色装备" then
					SetRecordAddress( rsd, string.format( "+07A0+0A*%02X", id ) )
				elseif rsd.Description == "角色数据" then
					SetRecordAddress( rsd, string.format( "+20+3C*%02X", id ) )
				end
			end

		else
			SetRecordOptions( Part, HeadOptions.AlwaysHide )
            Part.Active = false
            Part.Collapsed = true

			if id == 0xFF then
				-- 无角色
				SetRecordDesc( Part, "---" )
				SetRecordColor( Part, 0 )

			else
				-- 友方：id = 0x10 - 0xFE
				local name = RoleSerialList[id] or MsIdList[id] or string.format( "ID = 0x%02X", id  )
				SetRecordDesc( Part, string.format( "%02d [NPC]%s", index, name ) )
				SetRecordColor( Part, 0x00FF0000 )

				if not RoleSerialList[id] and not MsIdList[id] then
					RoleSerialList[id] = string.format( "ID = 0x%02X", id  )
					local str = string.format( "未知NPC，ID = 0x%02X", id )
					print( "\r\n===================================" )
					print( str )
					SendSound( "未知NPC" )
				end
			end
		end
	end

	-- 队伍成员变动断点处理
	-- 		+F0444		ecx = 地址，	edi = 角色ID
	-- 		+F045B		ecx = 地址，	eax = 角色ID		清除
	-- 		+F03A0		队伍全部清除
	local function OnTimer()
		local bad = GetBaseAddress()
		if bad then
			local sad = bad + 0x28A518C
			-- 刷新全部
			for i = 1, 8 do
				local id = readInteger( sad )
				PartChanged( i, id )
				sad = sad + 4
			end
		end

	end

	-- 计时器对象
	PartMemberTimer = CombatTimer or createTimer( getMainForm() )
	PartMemberTimer.Interval = 500
	PartMemberTimer.OnTimer = OnTimer

	OnTimer()

{$asm}

[DISABLE]

{$lua}

	-- 销毁计时器
	if PartMemberTimer then
		PartMemberTimer.destroy()
		PartMemberTimer = nil
	end

{$asm}


