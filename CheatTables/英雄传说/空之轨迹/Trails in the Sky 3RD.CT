    <CheatEntry>
      <ID>3851</ID>
      <Description>"战斗数据"</Description>
      <Options moHideChildren="1" moManualExpandCollapse="1" moDeactivateChildrenAsWell="1"/>
      <Color>FF00FF</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
//从此处开始直到 '[DISABLE]' 的代码将被用于启用功能

{$lua}

	do -- 记录栏：CombatMemRecords = { index = RCD }

		-- 战斗时角色数据记录栏
		CombatMemRecords = {}
		-- 初始化记录栏地址
		local list = getAddressList()
		if list then
			local index = 1
			-- 我方角色=3852, 友方角色=4917, 敌方角色=4918
			for _, headid in ipairs( { 3852, 4917, 4918 } ) do
				local head = list.getMemoryRecordByID( headid )
				if head then
					for idx = 0, head.Count - 1 do
						table.insert( CombatMemRecords, head[idx] )
						SetRecordAddress( head[idx], string.format( "+2490*%02X", index - 1 ) )
						index = index + 1
					end
				end
			end
		end

	end

	-- 标志：是否战斗中
	local Combating = false

	-- 清除记录栏
	local function ClearRecords()
		for index, rcd in pairs( CombatMemRecords ) do
			if rcd then
				SetRecordDesc( rcd, string.format( "%02d ---", index ) )
				SetRecordColor( rcd, 0 )
			end
		end
		RoleMsList[0] = nil
	end

	-- 检测状态并警告
	local function CheckState( index, state )
		if state &amp; ConfirmedState ~= state then
			-- 我方：01-04、友方：05-08、敌方：09-17、我方召唤：18-19，敌方替补：20-21
			local idx = index &lt; 5 and index or index &lt; 9 and index-4 or index - 8
			local str = index &lt; 5 and "我方" or index &lt; 9 and "友方" or index &lt; 18 and "敌方" or index &lt; 20 and "我方召唤" or "敌方（替补）"

			local sName = StateNameList[state] or ""
			sName = sName ~= "" and ( " 参考状态 = " .. sName ) or ""
			SendSound( "未知状态" .. sName )

			print( "\r\n===================================" )
			print( string.format( "未知状态, %s：No.%02d,  Code = %08X,  %s", str, idx, ~( ConfirmedState &amp; state ) &amp; state, sName ) )

			ConfirmedState = ConfirmedState | state
			print( string.format( "更新状态掩码： %08X", ConfirmedState ) )

		end
	end

	-- 更新全部战斗时角色栏
	local function RefreshAll( force )
		local bad = GetBaseAddress()

		if not bad then
			if force then ClearRecords() end
			return
		end

		-- 检测战斗标志
		local val = readByte( bad + 0x2951824 + 2 )
		if val &amp; 0x04 == 0 then
			-- 退出战斗时
			if Combating then
				Combating = false
				ClearRecords()
			end
			return
		end

		Combating = true
		local sad = bad + CombatStartAddressOffset
		for index, rcd in ipairs( CombatMemRecords ) do
			if rcd then
				-- 我方：1-4，NPC：5-8，敌方：09-17，我方召唤：18-19，敌方替补：21 → 20
				local idx = index &lt; 5 and index or index &lt; 9 and index-4 or index - 8
				local camp = readSmallInteger( sad + 2 )	-- 阵营
				if camp ~= 0 and camp ~= 0x8000 then
					-- 存在阵营：角色存在
					local state = readInteger( sad + 0x029C )		-- 状态
					local mid = readSmallInteger( sad + 0x14 )		-- 角色MSID

					local fix = ( state &amp; 0x80000000 ~= 0 ) and "【战斗不能】" or index &gt;= 20 and "【替补】" or ""
					local name
					if mid == 0 then
						name = camp == 0x4000 and "【我方召唤】基库" or camp == 0x1000 and "约修亚分身" or string.format( "ID：%04X", mid )
					else
						name = RoleMsList[mid] or string.format( "ID：%04X", mid )
					end
					SetRecordDesc( rcd, string.format( "%02d %s%s", idx, fix, name ) )
					SetRecordColor( rcd, fix == "【战斗不能】" and 0x0000FF or 0 )

					-- 检测未知角色/未知状态
					if not RoleMsList[mid] then
						RoleMsList[mid] = string.format( "ID：%04X", mid )
						local str = index &lt; 5 and "我方" or index &lt; 9 and "友方" or index &lt; 18 and "敌方" or index &lt; 20 and "我方召唤" or "敌方（替补）"

						print( "\r\n===================================" )
						print( string.format( "未知角色，%s：No.%02d， ID = %04X", str, idx, mid ) )
						SendSound( "未知角色" )
					end

					-- 状态
					CheckState( index, state )

				else
					-- 无阵营
					SetRecordDesc( rcd, string.format( "%02d ---", idx ) )
					SetRecordColor( rcd, 0 )
				end
			end

			sad = sad + 0x2490
		end

	end

	-- 获得状态时断点处理
		-- main.exe+1BDD7 - mov edx,[ebp+0000029C]
		-- main.exe+1BDDD - or edx,eax
		-- main.exe+1BDDF - cmp eax,00000200
		-- main.exe+1BDE4 - mov [ebp+0000029C],edx		ebp=角色数据起始地址，	eax=新增的状态掩码
		-- main.exe+1BDEA - jne main.exe+1BDF5			&lt; 断点
	local function OnBreakState()
		local bad = GetBaseAddress()
		if not bad then return end

		local sad = bad + CombatStartAddressOffset
		local index =  (EBP - sad ) / 0x2490 + 1
		CheckState( index, EAX )
	end

	-- 战斗时角色栏名称
	local bad = GetBaseAddress()
	if bad then
		-- 计时器对象
		Sora3CombatCharTimer = CombatTimer or createTimer( getMainForm() )
		-- Sora3CombatCharTimer.Enabled = false
		Sora3CombatCharTimer.Interval = 500
		Sora3CombatCharTimer.OnTimer = RefreshAll

		-- 设置断点
        AddBreakPoint( bad + 0x1BDEA, OnBreakState )	-- 获得状态

	end

	RefreshAll( true )

{$asm}

[DISABLE]
//从此处开始直到末尾的代码将被用于禁用功能

{$lua}

	-- 移除断点
	local bad = GetBaseAddress()
	if bad then
		RemoveBreakPoint( bad + 0x1BDEA )
	end

	-- 销毁计时器
	if Sora3CombatCharTimer then
		Sora3CombatCharTimer.destroy()
		Sora3CombatCharTimer = nil
	end

	-- 恢复记录栏的显示说明到默认值
	if CombatMemRecords then
		for index, rcd in ipairs( CombatMemRecords ) do
			if rcd then
				SetRecordDesc( rcd, string.format( "%02d ---", index ) )
				SetRecordColor( rcd, 0 )
			end
		end
	end

{$asm}


