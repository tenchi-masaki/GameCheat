  经验值倍增
}

[ENABLE]

aobscanmodule(ExpMultiple_AOB,ed6_win2_DX9_cn.exe,2B C8 0F B7 C7) // should be unique
alloc(newmem,$1000,ExpMultiple_AOB)

label(code)
label(ExpMultipleAD)
label(return)

newmem:
  // 4000=我方，6000=NPC，1000=敌方，8000=---
  test word ptr [edx+02], 4000
  jz code
  // 00FF = 非操作角色（不含友方），判断首个角色是否为我方控制角色
  cmp byte ptr [main.exe+272828+0A], FF
  je code

  push eax
  push edx
  push ecx

  mov ecx, [ExpMultipleAD]
  cmp ecx, 32					// 倍率最大=50
  jbe @f
  mov ecx, 32
@@:
  cmp ecx, 01					// 倍率最小=2
  ja @f
  mov ecx, 02
@@:
  mov [ExpMultipleAD], ecx

  mov eax, edi
  xor edx, edx
  mul ecx
  mov edi, eax

  pop ecx
  pop edx
  pop eax

code:
  sub ecx,eax
  movzx eax,di
  jmp return

  align 04    // 按提供的数值对齐内存地址，如有第二个参数则以此参数数值填充，否则填充0
ExpMultipleAD:
  dd 0032

ExpMultiple_AOB:
  jmp newmem
return:
registersymbol(ExpMultipleAD)
registersymbol(ExpMultiple_AOB)

