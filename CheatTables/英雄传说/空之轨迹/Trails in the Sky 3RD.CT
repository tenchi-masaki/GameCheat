do	-- 使用断点方式以 Lua 代码处理事务
	--[[
		调试时可用的寄存器变量：
		只有当进程中断并且没有（其他）当前 debug_setBreakpoint 方法运行时，对这些变量的更改才会生效。
			标志寄存器：	EFLAGS
			32位: 			EAX, EBX, ECX, EDX, EDI, ESI, EBP, ESP, EIP
			64 位: 			RAX, RBX, RCX, RDX, RDI, RSI, RBP, RSP, RIP
							R8, R9, R10, R11, R12, R13, R14, R15
			浮点：			FP0, FP1, FP2, FP3, FP4, FP5, FP6, FP7
			XMM-32：		XMM0, XMM1, XMM2, XMM3, XMM4, XMM5, XMM6, XMM7
			XMM-64：		XMM8, XMM9, XMM10, XMM11, XMM12, XMM13, XMM14, XMM15

		function debugger_onBreakpoint()		当断点命中（包括单步执行）并且定义了此函数时，将调用它
												返回值： 0 = 继续执行代码， 1 = 在断点处中断

		debug_isDebugging()						如果调试器已启动，则返回true
		debugProcess( [Interface] )				启动当前附加进程的调试器。
													0=default，1=windows，2=VEH，3=Kernel
		debug_getCurrentDebuggerInterface()		返回当前使用的调试接口
													1=windows，2=VEH，3=内核，nil=无调试活动

		debug_canBreak()						如果目标可能在断点上停止，则返回true
		debug_isBroken()						如果调试器当前在线程上暂停，则返回true

		debug_getBreakpointList()				返回包含所有断点地址的 Lua 表

		debug_setBreakpoint( address, size, trigger, method, function )
			size    ：	从地址开始计数要中断的字节数， trigger == ：bptExecute 时忽略此参数
			trigger ：	bptExecute = 执行时中断，bptAccess = 访问时中断， bptWrite = 写入时中断
			method  ：
						bpmInt3 = 0				软中断（无限制）
						bpmDebugRegister = 1	硬中断（4个）
						bpmException = 2		bpmException = 异常（问题比较多，不建议用）
						bpmDBVM=3,
						bpmDBVMNative=4,
						bpmGDB=5
			function：	中断后执行的方法。
						方法返回值：
							1		表示已处理了断点（如LUA代码中未调用 debug_continueFromBreakpoint 会导致中断）
							0		表示忽略断点，代码继续执行下去
		debug_removeBreakpoint( address )	移除指定地址的断点

		debug_continueFromBreakpoint(method)
			method： 常数参数
						co_run = 0			恢复正常执行。
						co_stepinto = 1		下一条指令；进入函数调用。
						co_stepover = 2		下一条指令；步过函数调用。
						co_runtill = 3		运行到给定的地址。
												在Lua中不可用，因为无法提供地址。
												可以通过在该地址设置断点，然后使用“corun”来模拟。

	]]

	-- 中断触发方式
	local Trigger = {
		[bptExecute] = bptExecute,		-- 执行时中断
		[bptAccess] = bptAccess,		-- 访问时中断
		[bptWrite] = bptWrite,			-- 写入时中断
	}

	-- 返回符合条件的断点数量，不检查参数！！
	local function GetBreakPointCount( address )
		local count = 0
		local breaks = debug_getBreakpointList()
		if type( breaks ) == "table" then
			for _, pad in ipairs( breaks ) do
				if pad == address then
					count = count + 1
				end
			end
		end

		return count
	end

	-- 检测调试器是否已启用
	function IsDebugging()
		return debug_isDebugging()
	end

	-- 添加断点，默认使用软中断，并且中断调用处理后继续执行代码
	-- 中断时，断点处语句还未执行！
	--		trigger ： bptExecute （执行时，默认）， bptAccess （访问时）， bptWrite （写入时）
	--		size    ： 默认 1 字节
	function AddBreakPoint( address, func, trigger, size )
		-- 调试器未启用时，自动启用
		if not debug_isDebugging() then
			debugProcess()
		end

		trigger = Trigger[trigger] or bptExecute
		size = size or 1

		if debug_isDebugging() and type(func) == "function" and type(address) == "number" and size &gt; 0 then
			local fun = function()
				func()
				return 0
			end

			-- 避免重复断点
			RemoveBreakPoint( address )
			-- 添加断点
			debug_setBreakpoint( address, size, trigger, bpmInt3, fun )
		end
	end

	-- 移除指定地址的断点
	function RemoveBreakPoint( address )
		local count = debug_isDebugging() and type(address) == "number" and GetBreakPointCount( address )
		for i = 1, count do
			debug_removeBreakpoint( address )
		end
	end

	-- 检测断点是否已设置，
	function IsExistBreakPoint( address )
		local count = debug_isDebugging() and type(address) == "number" and GetBreakPointCount( address )
		return count &gt; 0
	end

end

